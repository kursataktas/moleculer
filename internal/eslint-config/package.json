{
  "name": "@moleculer/eslint-config",
  "version": "1.0.0",
  "description": "ESLint configuration of Moleculer framework packages",
  "type": "module",
  "exports": {
    "./nodejs.mjs": "./nodejs.mjs"
  },
  "scripts": {
    "prettier": "prettier . --ignore-unknown --check",
    "prettier:fix": "prettier . --ignore-unknown --write",
    "lint": "TIMING=1 eslint ."
  },
  "peerDependencies": {
    "eslint": "catalog:"
  },
  "dependencies": {
    "@eslint/js": "9.13.0",
    "@moleculer/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "typescript-eslint": "8.10.0"
  },
  "devDependencies": {
    "@types/eslint-config-prettier": "6.11.3",
    "@types/eslint__js": "8.42.3",
    "prettier": "catalog:"
  }
}
